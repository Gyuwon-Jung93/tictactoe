    tic-tac-toe:moveTakenBy tic-tac-toe:OPlayerRole ;
    tic-tac-toe:nextMove <http://localhost:8083/Square31/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

<http://localhost:8083/Square21/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Square21,
        owl:NamedIndividual ;
    tic-tac-toe:moveTakenBy tic-tac-toe:OPlayerRole ;
    tic-tac-toe:nextMove <http://localhost:8083/Square22/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

<http://localhost:8083/Square22/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Square22,
        owl:NamedIndividual ;
    tic-tac-toe:moveTakenBy tic-tac-toe:XPlayerRole ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

<http://localhost:8083/Square23/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Square23,
        owl:NamedIndividual ;
    tic-tac-toe:moveTakenBy tic-tac-toe:XPlayerRole ;
    tic-tac-toe:nextMove <http://localhost:8083/Square13/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

<http://localhost:8083/Square31/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Square31,
        owl:NamedIndividual ;
    tic-tac-toe:moveTakenBy tic-tac-toe:XPlayerRole ;
    tic-tac-toe:nextMove <http://localhost:8083/Square21/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

<http://localhost:8083/Square32/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Square32,
        owl:NamedIndividual ;
    tic-tac-toe:moveTakenBy tic-tac-toe:OPlayerRole ;
    tic-tac-toe:nextMove <http://localhost:8083/Square23/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

<http://localhost:8083/Square33/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Square33,
        owl:NamedIndividual ;
    tic-tac-toe:moveTakenBy tic-tac-toe:XPlayerRole ;
    tic-tac-toe:nextMove <http://localhost:8083/Square11/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

tic-tac-toe:Agent a owl:Class ;
    owl:disjointWith tic-tac-toe:AgentRole,
        tic-tac-toe:Square .

tic-tac-toe:Square11 a owl:Class ;
    rdfs:comment "Represents the top left hand square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square12 a owl:Class ;
    rdfs:comment "Represents the top middle square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square13 a owl:Class ;
    rdfs:comment "Represents the top right hand square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square21 a owl:Class ;
    rdfs:comment "Represents the middle left hand square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square22 a owl:Class ;
    rdfs:comment "Represents the middle square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square23 a owl:Class ;
    rdfs:comment "Represents the middle right hand square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square31 a owl:Class ;
    rdfs:comment "Represents the bottom left hand square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square32 a owl:Class ;
    rdfs:comment "Represents the bottom middle square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:Square33 a owl:Class ;
    rdfs:comment "Represents the bottom right hand square in a tic-tac-toe board"@en ;
    rdfs:subClassOf tic-tac-toe:Square .

tic-tac-toe:providesAgentRole a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:Game ;
    rdfs:range tic-tac-toe:AgentRole .

tic-tac-toe:subEventOf a owl:AsymmetricProperty,
        owl:FunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:Move ;
    rdfs:range tic-tac-toe:Game .

tic-tac-toe:Move a owl:Class ;
    rdfs:seeAlso "The event in a game of a player (either in the X Player Role or O Player Role) placing their mark (X or O) in a square"@en ;
    rdfs:subClassOf tic-tac-toe:Game ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onClass tic-tac-toe:Square ;
                        owl:onProperty tic-tac-toe:inSquare ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onClass tic-tac-toe:AgentRole ;
                        owl:onProperty tic-tac-toe:moveTakenBy ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onClass tic-tac-toe:Game ;
                        owl:onProperty tic-tac-toe:subEventOf ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] .

tic-tac-toe:OPlayerRole a owl:Class ;
    rdfs:subClassOf tic-tac-toe:AgentRole ;
    owl:disjointWith tic-tac-toe:XPlayerRole .

tic-tac-toe:AgentRole a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass tic-tac-toe:Agent ;
            owl:onProperty tic-tac-toe:performedBy ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
    owl:disjointWith tic-tac-toe:Square .

tic-tac-toe:Game a owl:Class ;
    rdfs:comment "A tic-tac-toe game, with 2 players performing O Player Role and X Player Role, whereby players take it in turn to perform a move in a square of the board until one player has a row of 3 marks either diagonally, vertically or horizontally, or there is no winner and no move free squares in which to move (a draw)."@en ;
    rdfs:seeAlso "http://dbpedia.org/resource/Tic-tac-toe"@en ;
    rdfs:subClassOf tic-tac-toe:Event ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onClass tic-tac-toe:OPlayerRole ;
                        owl:onProperty tic-tac-toe:providesAgentRole ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] [ a owl:Restriction ;
                        owl:onClass tic-tac-toe:XPlayerRole ;
                        owl:onProperty tic-tac-toe:providesAgentRole ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ) ] .

tic-tac-toe:XPlayerRole a owl:Class ;
    rdfs:subClassOf tic-tac-toe:AgentRole .

<http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Game,
        owl:NamedIndividual ;
    tic-tac-toe:firstMove <http://localhost:8083/Square33/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:hasID "12addd11-172a-42e2-9bf3-f6bc0c25c163" ;
    tic-tac-toe:providesAgentRole <http://127.0.0.1:9000/Better>,
        <http://127.0.0.1:9000/NewPlayer> .

tic-tac-toe:Square a owl:Class .

[] a owl:AllDisjointClasses ;
    owl:members ( tic-tac-toe:Square11 tic-tac-toe:Square12 tic-tac-toe:Square13 tic-tac-toe:Square21 tic-tac-toe:Square22 tic-tac-toe:Square23 tic-tac-toe:Square31 tic-tac-toe:Square32 tic-tac-toe:Square33 ) .

