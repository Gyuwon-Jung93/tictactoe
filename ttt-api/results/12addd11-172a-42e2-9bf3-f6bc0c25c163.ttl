<http://localhost:8083/tic-tac-toe> a owl:Ontology ;
    rdfs:label "Tic-tac-toe ontology" ;
    rdfs:seeAlso "https://dbpedia.org/page/Tic-tac-toe" .

tic-tac-toe:hasFirstMove a owl:ObjectProperty ;
    rdfs:subPropertyOf tic-tac-toe:hasMove .

tic-tac-toe:hasID a owl:AsymmetricProperty,
        owl:FunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:Game .

tic-tac-toe:hasResult a owl:AsymmetricProperty,
        owl:FunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:Game .

tic-tac-toe:hasSquare a owl:ObjectProperty ;
    rdfs:range tic-tac-toe:Square .

tic-tac-toe:nextMove a owl:AsymmetricProperty,
        owl:FunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:Move ;
    rdfs:range tic-tac-toe:Move ;
    rdfs:subPropertyOf owl:topObjectProperty .

<http://127.0.0.1:9000/Better> a tic-tac-toe:OPlayerRole .

<http://127.0.0.1:9000/NewPlayer> a tic-tac-toe:XPlayerRole .

tic-tac-toe:Board a owl:Class ;
    rdfs:comment "Descriptive placeholder for querying the state of all of the squares in the game"^^xsd:string .

tic-tac-toe:Event a owl:Class .

tic-tac-toe:TicTacToeResult a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:domain [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty tic-tac-toe:TicTacToeResult ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ] ;
    rdfs:range xsd:string .

tic-tac-toe:hasMove a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:AgentRole,
        tic-tac-toe:Game ;
    rdfs:range tic-tac-toe:Move ;
    owl:inverseOf tic-tac-toe:subEventOf .

tic-tac-toe:inSquare a owl:AsymmetricProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:Move ;
    rdfs:range tic-tac-toe:Square .

tic-tac-toe:moveTakenBy a owl:AsymmetricProperty,
        owl:FunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:Move ;
    rdfs:range tic-tac-toe:AgentRole .

tic-tac-toe:performedBy a owl:AsymmetricProperty,
        owl:FunctionalProperty,
        owl:IrreflexiveProperty,
        owl:ObjectProperty ;
    rdfs:domain tic-tac-toe:AgentRole ;
    rdfs:range tic-tac-toe:Agent .

<http://localhost:8083/Square11/12addd11-172a-42e2-9bf3-f6bc0c25c163> a tic-tac-toe:Square11,
        owl:NamedIndividual ;
    tic-tac-toe:moveTakenBy tic-tac-toe:OPlayerRole ;
    tic-tac-toe:nextMove <http://localhost:8083/Square12/12addd11-172a-42e2-9bf3-f6bc0c25c163> ;
    tic-tac-toe:subEventOf <http://localhost:8083/Game/12addd11-172a-42e2-9bf3-f6bc0c25c163> .

